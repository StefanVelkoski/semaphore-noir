{
    "name": "@semaphore-protocol/proof",
    "version": "0.0.1",
    "description": "A library to generate and verify Semaphore proofs with Aztec's noir.",
    "license": "MIT",
    "main": "dist/index.node.js",
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.node.js"
    },
    "types": "dist/types/index.d.ts",
    "files": [
        "dist/",
        "src/",
        "LICENSE",
        "README.md"
    ],
    "repository": "https://github.com/semaphore-protocol/semaphore",
    "scripts": {
        "build:watch": "rollup -c rollup.config.ts -w --configPlugin typescript",
        "build": "rimraf dist && rollup -c rollup.config.ts --configPlugin typescript",
        "test": "yarn vitest",
        "test:index": "yarn hardhat test ./src/index.test.ts",
        "prepublishOnly": "yarn build",
        "docs": "typedoc src/index.ts --out ../../docs/proof"
    },
    "installConfig": {
        "hoistingLimits": "workspaces"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^24.1.0",
        "@rollup/plugin-json": "^5.0.1",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "ffjavascript": "^0.2.54",
        "poseidon-lite": "^0.2.0",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-typescript2": "^0.31.2",
        "typedoc": "^0.22.11",
        "vitest": "^0.34.2"
    },
    "dependencies": {
        "@aztec/bb.js": "^0.8.14",
        "@ethersproject/bignumber": "^5.5.0",
        "@ethersproject/bytes": "^5.7.0",
        "@ethersproject/keccak256": "^5.7.0",
        "@ethersproject/strings": "^5.5.0",
        "@noir-lang/backend_barretenberg": "0.18.0",
        "@noir-lang/noir_js": "0.18.0",
        "@noir-lang/types": "0.18.0",
        "@semaphore-protocol/group": "3.11.0",
        "@semaphore-protocol/identity": "3.11.0",
        "@signorecello/backend_barretenberg": "^0.19.6",
        "@zk-kit/incremental-merkle-tree": "0.4.3",
        "rimraf": "^5.0.1",
        "rollup": "^3.28.1",
        "snarkjs": "0.4.16"
    }
}
